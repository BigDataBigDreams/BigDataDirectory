class DataSet:
	def __init__(self, name, keys, pref=''):
		self.keys = keys
		self.pref = pref
		
	def createString(self):
		s = """create table %(name) 
		(
			%(pref)_id int NOT NULL,
			%(pref)_brand varchar(32),
			%(pref)_model varchar(32),
			%(pref)_star tinyint,
			%(pref)_power decimal(4,2),
			PRIMARY KEY (%(pref)_id)
		);"""
		
	def colArray(fPath):
		cols = {}
		for key, value in self.keys:
			cols[key] = []
